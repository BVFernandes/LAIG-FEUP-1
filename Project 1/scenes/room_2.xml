<SCENE>

  <!-- Initial values -->
  <INITIALS>
    <frustum near="0.1" far="500"/>
    <translation x="-1" y="1" z="0" />
    <rotation axis="x" angle="0" />
    <rotation axis="y" angle="0" />
    <rotation axis="z" angle="0" />
    <scale sx="0.25" sy="0.25" sz="0.25" />
    <reference length="5" />
  </INITIALS>


  <!-- Illumination and clear colour -->
  <ILLUMINATION>
    <ambient r="0.2" g="0.2" b="0.2" a="1" />
    <background r="0" g="0" b="0.7" a="1" /> //blue like the submarine in cgra tp6
  </ILLUMINATION>


  <!-- Lights -->
  <LIGHTS>

    <!-- light in center of the scene -->
    <LIGHT id="lamp_light">
      <enable value ="1" />
      <position  x="7.5" y="5" z="7.5" w="1" />
      <ambient r="1" g="1" b="1" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <!-- Light in lamp stand -->
    <LIGHT id="lamp_stand_light">
      <enable value ="1" />
      <position  x="13.5" y="3" z="13.5" w="1" />
      <ambient r="1" g="1" b="1" a="0.1" />
      <diffuse r="1.0" g="1.0" b="1.0" a="0.01" />
      <specular r="1.0" g="1.0" b="1.0" a="0.01" />
    </LIGHT>

    <!-- Light in fireplace-->
    <LIGHT id="fireplace_light">
      <enable value ="0" />
      <position  x="12" y="1.5" z="5" w="1" />
      <ambient r="0.1" g="0.1" b="0.1" a="1" />
      <diffuse r="1.0" g="0.0" b="0.0" a="1" />
      <specular r="1.0" g="0.0" b="0.0" a="1" />
    </LIGHT>

  </LIGHTS>


  <!-- Textures -->
  <TEXTURES>

    <!-- Leather -->
    <TEXTURE id="leatherTexture">
      <file path="images_room/leather.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Painting 1 -->
    <TEXTURE id="painting1Texture">
      <file path="images_room/painting1.jpg" />
      <amplif_factor s="1.6" t="2.1" />
    </TEXTURE>

    <!-- Painting 2 -->
    <TEXTURE id="painting2Texture">
      <file path="images_room/painting2.jpg" />
      <amplif_factor s="1.6" t="2.1" />
    </TEXTURE>

    <!-- Wood Floor -->
    <TEXTURE id="woodFloorTexture">
      <file path="images_room/woodFloor.jpg" />
      <amplif_factor s="0.1" t="0.1" />
    </TEXTURE>

    <!-- Wood -->
    <TEXTURE id="woodTexture">
      <file path="images_room/wood.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Carpet -->
    <TEXTURE id="carpetTexture">
      <file path="images_room/carpet.jpg" />
      <amplif_factor s="0.1" t="0.1" />
    </TEXTURE>

    <!-- Fireplace Rock -->
    <TEXTURE id="rockTexture">
      <file path="images_room/fireplace.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Fire -->
    <TEXTURE id="fireTexture">
      <file path="images_room/fire.jpg" />
      <amplif_factor s="3" t="2.4" />
    </TEXTURE>

    <!-- Bricks (Right Wall) -->
    <TEXTURE id="bricksTexture">
      <file path="images_room/brick.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Left Wall -->
    <TEXTURE id="wall1Texture">
      <file path="images_room/wall.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Television Screen -->
    <TEXTURE id="lionkingTexture">
      <file path="images_room/reileao1.jpg" />
      <amplif_factor s="2" t="1" />
    </TEXTURE>

    <!-- Speaker -->
    <TEXTURE id="speakerTexture">
      <file path="images_room/speaker.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Lamp -->
    <TEXTURE id="lampTexture">
      <file path="images_room/lamp.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>


  </TEXTURES>


  <!-- Materials -->
  <MATERIALS>

    <!-- A default material -->
    <MATERIAL id="defaultMaterial">
      <shininess value="1" />
      <specular r="0.0" g="0.0" b="0.0" a="1" />
      <diffuse r="0.4" g="0.4" b="0.4" a="1" />
      <ambient r="0.4" g="0.4" b="0.4" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Metal material -->
    <MATERIAL id="metalMaterial">
      <shininess value="255" />
      <specular r="1" g="1" b="1" a="0.5" />
      <diffuse r="0.6" g="0.6" b="0.6" a="1" />
      <ambient r="0.3" g="0.3" b="0.3" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Floor material -->
    <MATERIAL id="floorMaterial">
      <shininess value="50" />
      <specular r="0.5" g="0.5" b="0.5" a="0.5" />
      <diffuse r="0" g="0" b="0" a="1" />
      <ambient r="0.3" g="0.3" b="0.3" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Wall material -->
    <MATERIAL id="wallMaterial">
      <shininess value="50" />
      <specular r="0" g="0" b="0" a="0.5" />
      <diffuse r="0" g="0" b="0" a="1" />
      <ambient r="1" g="1" b="1" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Plastic material -->
    <MATERIAL id="plasticMaterial">
      <shininess value="50" />
      <specular r="0.1" g="0.1" b="0.1" a="1" />
      <diffuse r="0.1" g="0.1" b="0.1" a="1" />
      <ambient r="0.1" g="0.1" b="0.1" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Glass material -->
    <MATERIAL id="glassMaterial">
      <shininess value="255" />
      <specular r="1" g="1" b="1" a="1" />
      <diffuse r="0" g="0" b="0" a="1" />
      <ambient r="0.5" g="0.5" b="0.5" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Leather material -->
    <MATERIAL id="leatherMaterial">
      <shininess value="50" />
      <specular r="0" g="0" b="0" a="1" />
      <diffuse r="0.5" g="0.5" b="0.5" a="1" />
      <ambient r="0.5" g="0.5" b="0.5" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- A black material -->
    <MATERIAL id="blackMaterial">
      <shininess value="1" />
      <specular r="0" g="0" b="0" a="1" />
      <diffuse r="0" g="0" b="0" a="1" />
      <ambient r="0.05" g="0.05" b="0.05" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- A black2 material -->
    <MATERIAL id="black2Material">
      <shininess value="0.1" />
      <specular r="0.0" g="0.0" b="0.0" a="1" />
      <diffuse r="0.5" g="0.5" b="0.5" a="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Speaker material -->
    <MATERIAL id="speakerMaterial">
      <shininess value="1" />
      <specular r="0.1" g="0.1" b="0.1" a="1" />
      <diffuse r="0.3" g="0.3" b="0.3" a="1" />
      <ambient r="0.3" g="0.3" b="0.3" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

  </MATERIALS>


  <!-- The nodes -->
  <NODES>

    <ROOT id="root" />

    <!-- The root node -->
    <NODE id="root">
      <MATERIAL id="defaultMaterial" />
      <TEXTURE id="lionkingTexture" />

      <DESCENDANTS>
        <NODEREF id="floor" />
        <NODEREF id="walls" />
        <NODEREF id="floor_lamp" />
        <NODEREF id="couch" />
        <NODEREF id="table" />
        <NODEREF id="shelf" />
        <NODEREF id="television" />
        <NODEREF id="painting1" />
        <NODEREF id="painting2" />
        <NODEREF id="carpet" />
        <NODEREF id="fireplace" />
        <NODEREF id="speaker_system" />
        <NODEREF id="pt"/>
        <LEAF type="cylinder" args="5 10 1 20 20 1 1" />

      </DESCENDANTS>

    </NODE>

    <!-- The floor -->
    <NODE id="floor">
      <MATERIAL id="floorMaterial" />
      <TEXTURE id="woodFloorTexture" />

      <TRANSLATION x="7.5" y="0" z="7.5" />
      <ROTATION axis="x" angle="-90" />
      <SCALE sx="15" sy="15" sz="1" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="-0.5 0.5 0.5 -0.5" />
      </DESCENDANTS>

    </NODE>

    <!-- The walls -->
    <NODE id="walls">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <DESCENDANTS>
        <NODEREF id="wall_left" />
        <NODEREF id="wall_back" />
      </DESCENDANTS>

    </NODE>

    <!-- The Left Wall -->
    <NODE id="wall_left">
      <MATERIAL id="null" />
      <TEXTURE id="bricksTexture" />

      <TRANSLATION x="7.5" y="2.5" z="0" />

      <DESCENDANTS>
        <NODEREF id="wall" />
      </DESCENDANTS>

    </NODE>

    <!-- The Back Wall -->
    <NODE id="wall_back">
      <MATERIAL id="null" />
      <TEXTURE id="wall1Texture" />

      <TRANSLATION x="0" y="2.5" z="7.5" />
      <ROTATION axis="y" angle="90" />
      <ROTATION axis="z" angle="180" />

      <DESCENDANTS>
        <NODEREF id="wall" />
      </DESCENDANTS>
    </NODE>

    <!-- Wall -->
    <NODE id="wall">
      <MATERIAL id="wallMaterial" />
      <TEXTURE id="null" />

      <SCALE sx="15" sy="5" sz="0" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="-0.5 0.5 0.5 -0.5" />
      </DESCENDANTS>
    </NODE>

    <!-- Floor lamp -->
    <NODE id="floor_lamp">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="13.5" y="0" z="13.5" />

      <DESCENDANTS>
        <NODEREF id="lamp_bulb" />
        <NODEREF id="lamp_pole" />
        <NODEREF id="lamp_base" />
      </DESCENDANTS>

    </NODE>

    <!-- Floor lamp bulb -->
    <NODE id="lamp_bulb">
      <MATERIAL id="glassMaterial" />
      <TEXTURE id="lampTexture" />

      <TRANSLATION x="0" y="3" z="0" />
      <ROTATION axis="y" angle="180" />

      <DESCENDANTS>
        <LEAF type="sphere" args="0.8 20 20" />
      </DESCENDANTS>

    </NODE>

    <!-- Floor lamp pole -->
    <NODE id="lamp_pole">
      <MATERIAL id="plasticMaterial" />
      <TEXTURE id="null" />

      <SCALE sx="0.1" sy="0.8" sz="0.1" />
      <ROTATION axis="x" angle="-90" />

      <DESCENDANTS>
        <LEAF type="cylinder" args="4 1 1 20 20" />
      </DESCENDANTS>

    </NODE>

    <!-- Floor lamp base -->
    <NODE id="lamp_base">
      <MATERIAL id="plasticMaterial" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0" z="0" />
      <SCALE sx="0.4" sy="0.1" sz="0.4" />
      <ROTATION axis="x" angle="-90" />

      <DESCENDANTS>
        <LEAF type="sphere" args="2 20 20" />
      </DESCENDANTS>

    </NODE>

    <!-- Couch -->
    <NODE id="couch">
      <MATERIAL id="black2Material" />
      <TEXTURE id="leatherTexture" />

      <TRANSLATION x="13" y="0.5" z="7.5"/>

      <DESCENDANTS>
        <NODEREF id="couch_top" />
        <NODEREF id="couch_base" />
        <NODEREF id="couch_base_pillow" />
        <NODEREF id="couch_headboard" />
        <NODEREF id="couch_side_l" />
        <NODEREF id="couch_side_pillow_l" />
        <NODEREF id="couch_side_r" />
        <NODEREF id="couch_side_pillow_r" />
      </DESCENDANTS>

    </NODE>

    <!-- Couch Top -->
    <NODE id="couch_top">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="1" y="1" z="0" />
      <SCALE sx="1" sy="1" sz="5" />

      <DESCENDANTS>
        <NODEREF id="cube" />
      </DESCENDANTS>

    </NODE>

    <!--Couch Base  -->
    <NODE id="couch_base">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <SCALE sx="3" sy="1" sz="5" />

      <DESCENDANTS>
        <NODEREF id="cube" />
      </DESCENDANTS>

    </NODE>

    <!--Couch Base Pillow  -->
    <NODE id="couch_base_pillow">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="-0.5" y="0.5" z="-1.5" />
      <SCALE sx="2" sy="0.5" sz="3" />

      <DESCENDANTS>
        <LEAF type="cylinder" args="1 0.5 0.5 20 20" />
      </DESCENDANTS>

    </NODE>

    <!-- Couch Headboard -->
    <NODE id="couch_headboard">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="1" y="2" z="-2.5" />

      <DESCENDANTS>
        <LEAF type="cylinder" args="5 0.6 0.6 20 20" />
      </DESCENDANTS>

    </NODE>

    <!-- Couch Side Left -->
    <NODE id="couch_side_l">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="-0.5" y="1" z="2" />
      <SCALE sx="2" sy="1" sz="1" />

      <DESCENDANTS>
        <NODEREF id="cube" />
      </DESCENDANTS>

    </NODE>

    <!-- Couch Side Pillow Left -->
    <NODE id="couch_side_pillow_l">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="-0.5" y="1.5" z="1.505" />
      <SCALE sx="1" sy="0.25" sz="0.99" />

      <DESCENDANTS>
        <LEAF type="cylinder" args="1 1 1 20 20" />
      </DESCENDANTS>

    </NODE>

    <!-- Couch Side Right -->
    <NODE id="couch_side_r">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="-0.5" y="1" z="-2" />
      <SCALE sx="2" sy="1" sz="1" />

      <DESCENDANTS>
        <NODEREF id="cube" />
      </DESCENDANTS>

    </NODE>

    <!-- Couch Side Pillow Right -->
    <NODE id="couch_side_pillow_r">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="-0.5" y="1.5" z="-2.495" />
      <SCALE sx="1" sy="0.25" sz="0.99" />

      <DESCENDANTS>
        <LEAF type="cylinder" args="1 1 1 20 20" />
      </DESCENDANTS>

    </NODE>

    <!-- Table -->
    <NODE id="table">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="1.5" y="0" z="7.5" />

      <DESCENDANTS>
        <NODEREF id="table_top" />
        <NODEREF id="table_legs" />
      </DESCENDANTS>

    </NODE>

    <!-- Table Top -->
    <NODE id="table_top">
      <MATERIAL id="null" />
      <TEXTURE id="woodTexture" />

      <TRANSLATION x="0" y="1" z="0" />
      <SCALE sx="2.5" sy="0.5" sz="5"/>


      <DESCENDANTS>
        <NODEREF id="cube" />
      </DESCENDANTS>

    </NODE>

    <!-- Table Legs -->
    <NODE id="table_legs">
      <MATERIAL id="metalMaterial" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0" z="0" />

      <DESCENDANTS>
        <NODEREF id="table_legs1" />
        <NODEREF id="table_legs2" />
        <NODEREF id="table_legs3" />
        <NODEREF id="table_legs4" />
      </DESCENDANTS>

    </NODE>

    <!-- Table Legs1 -->
    <NODE id="table_legs1">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="-0.9" y="0" z="-2.2" />

      <DESCENDANTS>
        <NODEREF id="table_leg" />
      </DESCENDANTS>

    </NODE>

    <!-- Table Legs2 -->
    <NODE id="table_legs2">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0.9" y="0" z="-2.2" />

      <DESCENDANTS>
        <NODEREF id="table_leg" />
      </DESCENDANTS>

    </NODE>

    <!-- Table Legs3 -->
    <NODE id="table_legs3">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="-0.9" y="0" z="2.2" />

      <DESCENDANTS>
        <NODEREF id="table_leg" />
      </DESCENDANTS>

    </NODE>

    <!-- Table Legs4 -->
    <NODE id="table_legs4">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0.9" y="0" z="2.2" />

      <DESCENDANTS>
        <NODEREF id="table_leg" />
      </DESCENDANTS>

    </NODE>

    <!-- Table Leg -->
    <NODE id="table_leg">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <SCALE sx="0.2" sy="0.8" sz="0.2" />
      <ROTATION axis="x" angle="-90" />

      <DESCENDANTS>
        <LEAF type="cylinder" args="1.5 1 1 20 20" />
      </DESCENDANTS>

    </NODE>

    <!-- Shelf -->
    <NODE id="shelf">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="5" y="3" z="0.5"/>

      <DESCENDANTS>
        <NODEREF id="shelf_base" />
        <NODEREF id="shelf_panel_r" />
        <NODEREF id="shelf_panel_l" />
      </DESCENDANTS>

    </NODE>

    <!-- Shelf Base -->
    <NODE id="shelf_base">
      <MATERIAL id="null" />
      <TEXTURE id="woodTexture" />

      <SCALE sx="5" sy="0.5" sz="1"/>

      <DESCENDANTS>
        <NODEREF id="cube" />
      </DESCENDANTS>

    </NODE>

    <!-- Shelf Right Panel -->
    <NODE id="shelf_panel_r">
      <MATERIAL id="metalMaterial" />
      <TEXTURE id="null" />

      <TRANSLATION x="-2.55" y="0.25" z="-0.25"/>
      <SCALE sx="0.1" sy="1" sz="0.5" />

      <DESCENDANTS>
        <NODEREF id="cube" />
      </DESCENDANTS>

    </NODE>

    <!-- Shelf Left Panel -->
    <NODE id="shelf_panel_l">
      <MATERIAL id="metalMaterial" />
      <TEXTURE id="null" />

      <TRANSLATION x="2.55" y="0.25" z="-0.25"/>
      <SCALE sx="0.1" sy="1" sz="0.5" />

      <DESCENDANTS>
        <NODEREF id="cube" />
      </DESCENDANTS>

    </NODE>

    <!-- Television -->
    <NODE id="television">
      <MATERIAL id="blackMaterial" />
      <TEXTURE id="null" />

      <TRANSLATION x="2" y="2.25" z="7.5"/>
      <ROTATION axis="y" angle="90" />
      <SCALE sx="1.2" sy="2" sz="1" />

      <DESCENDANTS>
        <NODEREF id="tv_main" />
        <NODEREF id="tv_sec" />
        <NODEREF id="tv_thrd" />
        <NODEREF id="tv_screen" />
      </DESCENDANTS>

    </NODE>

    <!-- Television Main Box -->
    <NODE id="tv_main">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <SCALE sx="2" sy="1" sz="1" />

      <DESCENDANTS>
        <NODEREF id="cube" />
      </DESCENDANTS>

    </NODE>

    <!-- Television Second Box -->
    <NODE id="tv_sec">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0" z="-0.65"/>
      <SCALE sx="1.8" sy="0.8" sz="0.3" />

      <DESCENDANTS>
        <NODEREF id="cube" />
      </DESCENDANTS>

    </NODE>

    <!-- Television Third Box -->
    <NODE id="tv_thrd">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0" z="-1"/>
      <SCALE sx="1" sy="0.6" sz="0.4" />

      <DESCENDANTS>
        <NODEREF id="cube" />
      </DESCENDANTS>

    </NODE>

    <!-- Television Screen -->
    <NODE id="tv_screen">
      <MATERIAL id="defaultMaterial" />
      <TEXTURE id="lionkingTexture" />

      <TRANSLATION x="0" y="0" z="0.55"/>
      <ROTATION axis="z" angle="180"/>

      <DESCENDANTS>
        <LEAF  type="rectangle" args="-0.9 0.4 0.9 -0.4" />
      </DESCENDANTS>

    </NODE>

    <!-- Speaker system-->
    <NODE id="speaker_system">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <DESCENDANTS>
        <NODEREF id="speaker_left" />
        <NODEREF id="speaker_right" />
      </DESCENDANTS>
    </NODE>

    <!-- Speaker left-->
    <NODE id="speaker_left">
      <MATERIAL id="null" />
      <TEXTURE id="null" />


      <TRANSLATION x="0.5" y="0" z="14.5"/>
      <ROTATION axis="y" angle="20"/>

      <DESCENDANTS>
        <NODEREF id="speaker" />
      </DESCENDANTS>
    </NODE>

    <!-- Speaker right-->
    <NODE id="speaker_right">
      <MATERIAL id="null" />
      <TEXTURE id="null" />


      <TRANSLATION x="0.5" y="0" z="0.55"/>
      <ROTATION axis="y" angle="-20"/>

      <DESCENDANTS>
        <NODEREF id="speaker" />
      </DESCENDANTS>
    </NODE>

    <!-- Speaker-->
    <NODE id="speaker">
      <MATERIAL id="speakerMaterial" />
      <TEXTURE id="null" />

