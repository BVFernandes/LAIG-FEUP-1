<SCENE>

  <!-- Initial values -->
  <INITIALS>
    <frustum near="0.1" far="500"/>
    <translation x="-1" y="1" z="0" />
    <rotation axis="x" angle="0" />
    <rotation axis="y" angle="0" />
    <rotation axis="z" angle="0" />
    <scale sx="0.25" sy="0.25" sz="0.25" />
    <reference length="5" />
  </INITIALS>


  <!-- Illumination and clear colour -->
  <ILLUMINATION>
    <ambient r="0.2" g="0.2" b="0.2" a="1" />
    <background r="0" g="0" b="0" a="1" />
  </ILLUMINATION>


  <!-- Lights -->
  <LIGHTS>

    <LIGHT id="tretas0">
      <enable value ="1" />
      <position  x="-15" y="10" z="-15" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <LIGHT id="tretas1">
      <enable value ="1" />
      <position x="15" y="10" z="-15" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <LIGHT id="tretas2">
      <enable value ="1" />
      <position x="-15" y="10" z="15" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <LIGHT id="tretas3">
      <enable value ="1" />
      <position x="15" y="10" z="15" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
      <specular r="0.6" g="0.6" b="0.6" a="1.0" />
    </LIGHT>

	<LIGHT id="tretas4">
      <enable value ="1" />
      <position  x="0" y="8" z="-25" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>


  </LIGHTS>

  <!-- Textures -->
  <TEXTURES>
   <!-- Leather texture -->
    <TEXTURE id="leatherTexture">
      <file path="images_room/leather.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Painting 1 texture -->
    <TEXTURE id="painting1Texture">
      <file path="images_room/painting1.jpg" />
      <amplif_factor s="1.6" t="2.1" />
    </TEXTURE>

    <!-- Painting 2 texture -->
    <TEXTURE id="painting2Texture">
      <file path="images_room/painting2.jpg" />
      <amplif_factor s="1.6" t="2.1" />
    </TEXTURE>

    <!-- Wood Floor texture -->
    <TEXTURE id="woodFloorTexture">
      <file path="images_room/floor.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Wood texture -->
    <TEXTURE id="woodTexture">
      <file path="images_room/wood.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Carpet texture -->
    <TEXTURE id="carpetTexture">
      <file path="images_room/carpet.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Fireplace Rock texture-->
    <TEXTURE id="rockTexture">
      <file path="images_room/fireplace.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Fire texture -->
    <TEXTURE id="fireTexture">
      <file path="images_room/fire.jpg" />
      <amplif_factor s="3" t="2.4" />
    </TEXTURE>

    <!-- Wall texture -->
    <TEXTURE id="wallTexture">
      <file path="images_room/wall.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Television screen texture -->
    <TEXTURE id="lionkingTexture">
      <file path="images_room/reileao1.jpg" />
      <amplif_factor s="1.8" t="0.8" />
    </TEXTURE>

    <!-- Speaker texture -->
    <TEXTURE id="speakerTexture">
      <file path="images_room/speaker.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Lamp bulb texture -->
    <TEXTURE id="lampTexture">
      <file path="images_room/lamp.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Glass bowl texture -->
    <TEXTURE id="glassTexture">
      <file path="images_room/glass.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Vacuum cleaner texture -->
    <TEXTURE id="vacCleanerTexture">
      <file path="images_room/vaccumCleaner.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

	<!-- Board top texture -->
	<TEXTURE id="boardTopTexture">
      <file path="images_board/boardTop.jpg" />
      <amplif_factor s="1" t="1" />
	</TEXTURE>

  </TEXTURES>


  <MATERIALS>

	 <!-- Metal material -->
    <MATERIAL id="metalMaterial">
      <shininess value="255" />
      <specular r="1" g="1" b="1" a="0.5" />
      <diffuse r="0.6" g="0.6" b="0.6" a="1" />
      <ambient r="0.3" g="0.3" b="0.3" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

	 <!-- Dark material -->
    <MATERIAL id="darkMaterial">
      <shininess value="255" />
      <specular r="1" g="1" b="1" a="1" />
      <diffuse r="0" g="0" b="0" a="1" />
      <ambient r="0" g="0" b="0" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Pyramid material -->
    <MATERIAL id="piramideMaterial">
      <shininess value="1" />
      <specular r="0" g="0" b="0" a="1" />
      <diffuse r="1.0" g="0.68" b="0.4" a="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Another Floor material -->
    <MATERIAL id="floorMaterial">
      <shininess value="1" />
      <specular r="0" g="0" b="0" a="0" />
      <diffuse r="0" g="0" b="0" a="0" />
      <ambient r="1" g="1" b="1" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="0" />
    </MATERIAL>

    <!-- Wall material -->
    <MATERIAL id="wallMaterial">
      <shininess value="50" />
      <specular r="0" g="0" b="0" a="0.5" />
      <diffuse r="0" g="0" b="0" a="1" />
      <ambient r="1" g="1" b="1" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Plastic material -->
    <MATERIAL id="plasticMaterial">
      <shininess value="50" />
      <specular r="0.1" g="0.1" b="0.1" a="1" />
      <diffuse r="0.1" g="0.1" b="0.1" a="1" />
      <ambient r="0.1" g="0.1" b="0.1" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Glass material -->
    <MATERIAL id="glassMaterial">
      <shininess value="255" />
      <specular r="1" g="1" b="1" a="1" />
      <diffuse r="0" g="0" b="0" a="1" />
      <ambient r="0.5" g="0.5" b="0.5" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- A black material -->
    <MATERIAL id="blackMaterial">
      <shininess value="1" />
      <specular r="0" g="0" b="0" a="1" />
      <diffuse r="0" g="0" b="0" a="1" />
      <ambient r="0.05" g="0.05" b="0.05" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- A grey material -->
    <MATERIAL id="greyMaterial">
      <shininess value="0.1" />
      <specular r="0.0" g="0.0" b="0.0" a="1" />
      <diffuse r="0.5" g="0.5" b="0.5" a="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- A speaker material -->
    <MATERIAL id="speakerMaterial">
      <shininess value="1" />
      <specular r="0.1" g="0.1" b="0.1" a="1" />
      <diffuse r="0.3" g="0.3" b="0.3" a="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- A redWall material -->
    <MATERIAL id="redWallMaterial">
      <shininess value="0.1" />
      <specular r="0" g="0" b="0" a="1" />
      <diffuse r="0" g="0" b="0" a="1" />
      <ambient r="0.35" g="0" b="0.05" a="0.1" />
      <emission r="0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Wood material -->
    <MATERIAL id="woodMaterial">
      <shininess value="1" />
      <specular r="0.0" g="0.0" b="0.0" a="1" />
      <diffuse r="0" g="0" b="0" a="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- A default material -->
    <MATERIAL id="defaultMaterial">
      <shininess value="1" />
      <specular r="0.0" g="0.0" b="0.0" a="1" />
      <diffuse r="0.4" g="0.4" b="0.4" a="1" />
      <ambient r="0.4" g="0.4" b="0.4" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

  </MATERIALS>


  <!-- The animations -->
  <ANIMATIONS>

	 <ANIMATION id="robot_linear_anim1" type="linear" speed="3">
      <controlpoint xx="1.5" yy="0" zz="12"/>
      <controlpoint xx="1.5" yy="0" zz="2"/>
      <controlpoint xx="7" yy="0" zz="4"/>
    </ANIMATION>

    <ANIMATION id="robot_linear_anim2" type="linear" speed="3">
      <controlpoint xx="7" yy="0" zz="10"/>
      <controlpoint xx="14" yy="0" zz="11"/>
      <controlpoint xx="11" yy="0" zz="14"/>
      <controlpoint xx="7" yy="0" zz="11"/>
    </ANIMATION>

    <ANIMATION id="robot_linear_anim3" type="linear" speed="3">
      <controlpoint xx="7" yy="0" zz="3"/>
      <controlpoint xx="14" yy="0" zz="3"/>
      <controlpoint xx="11" yy="0" zz="4"/>
      <controlpoint xx="10" yy="0" zz="9"/>
    </ANIMATION>

    <ANIMATION id="robot_linear_anim4" type="linear" speed="3">
      <controlpoint xx="8" yy="0" zz="11"/>
      <controlpoint xx="1.5" yy="0" zz="12"/>
    </ANIMATION>

    <ANIMATION id="robot_circular_anim1" type="circular" speed="3" centerx="7" centery="0" centerz="7" radius="3" startang="90" rotang="-180" />

    <ANIMATION id="robot_circular_anim2" type="circular" speed="3" centerx="7" centery="0" centerz="7" radius="4" startang="-90" rotang="-180" />

    <ANIMATION id="robot_circular_anim3" type="circular" speed="3" centerx="8" centery="0" centerz="9" radius="2" startang="0" rotang="-90" />

    <ANIMATION id="plane_circular_anim1" type="circular" speed="15" centerx="7.5" centery="2" centerz="14" radius="1" startang="-90" rotang="-360" />

    <ANIMATION id="plane_circular_anim2" type="circular" speed="3" centerx="8" centery="0" centerz="9" radius="2" startang="0" rotang="-90" />

    <ANIMATION id="plane_bezier_anim1" type="bezier" speed="10">
      <controlpoint xx="5" yy="2" zz="3"/>
      <controlpoint xx="18" yy="10" zz="2.5"/>
      <controlpoint xx="20" yy="5" zz="18"/>
      <controlpoint xx="7.5" yy="2" zz="15"/>
    </ANIMATION>

    <ANIMATION id="plane_bezier_anim2" type="bezier" speed="10">
      <controlpoint xx="7.5" yy="2" zz="15"/>
      <controlpoint xx="0" yy="5" zz="13"/>
      <controlpoint xx="0" yy="7" zz="2.5"/>
      <controlpoint xx="5" yy="2" zz="3"/>
    </ANIMATION>

    <ANIMATION id="robot_combo_anim" type="combo">
      <SPANREF id="robot_linear_anim2"/>
      <SPANREF id="robot_circular_anim2"/>
      <SPANREF id="robot_linear_anim3"/>
    </ANIMATION>

  </ANIMATIONS>


  <!-- The nodes -->
  <NODES>

	<ROOT id="root" />

    <!-- The root node -->
    <NODE id="root">
      <MATERIAL id="defaultMaterial" />
      <TEXTURE id="null" />

      <DESCENDANTS>
        <NODEREF id="board" />
		    <NODEREF id="room" />
      </DESCENDANTS>
    </NODE>

    <!-- Ghost Board -->
      <NODE id="ghost_board">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <DESCENDANTS>
           <NODEREF id="board" />
  		 <NODEREF id="ghost_circles" />
        </DESCENDANTS>
      </NODE>


	<!-- Board -->
    <NODE id="board">
      <MATERIAL id="metalMaterial" />
      <TEXTURE id="null" />

      <SCALE sx="20" sy="1" sz="20" />

      <DESCENDANTS>
         <NODEREF id="boardCube" />
      </DESCENDANTS>
    </NODE>

	<!-- Unitary Cube -->
    <NODE id="boardCube">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <DESCENDANTS>
        <NODEREF id="boardFaceCube1" />
        <NODEREF id="boardFaceCube2" />
        <NODEREF id="boardFaceCube3" />
        <NODEREF id="boardFaceCube4" />
        <NODEREF id="boardFaceCube5" />
        <NODEREF id="boardFaceCube6" />
      </DESCENDANTS>
    </NODE>

    <!-- A cube face -->
    <NODE id="boardFaceCube1">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0" z="0.5" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="-0.5 0.5 0.5 -0.5" />
      </DESCENDANTS>
    </NODE>

    <!-- Another cube face -->
    <NODE id="boardFaceCube2">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <ROTATION axis="x" angle="180" />
      <TRANSLATION x="0" y="0" z="0.5" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="-0.5 0.5 0.5 -0.5" />
      </DESCENDANTS>
    </NODE>

    <!-- Another cube face -->
    <NODE id="boardFaceCube3">
      <MATERIAL id="null" />
      <TEXTURE id="boardTopTexture" />

      <ROTATION axis="x" angle="-90" />
      <TRANSLATION x="0" y="0" z="0.5" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="-0.5 0.5 0.5 -0.5" />
      </DESCENDANTS>
    </NODE>

    <!-- Another cube face -->
    <NODE id="boardFaceCube4">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <ROTATION axis="x" angle="90" />
      <TRANSLATION x="0" y="0" z="0.5" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="-0.5 0.5 0.5 -0.5" />
      </DESCENDANTS>
    </NODE>

    <!-- Another cube face -->
    <NODE id="boardFaceCube5">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <ROTATION axis="y" angle="-90" />
      <TRANSLATION x="0" y="0" z="0.5" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="-0.5 0.5 0.5 -0.5" />
      </DESCENDANTS>
    </NODE>

    <!-- Another cube face -->
    <NODE id="boardFaceCube6">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <ROTATION axis="y" angle="90" />
      <TRANSLATION x="0" y="0" z="0.5" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="-0.5 0.5 0.5 -0.5" />
      </DESCENDANTS>
    </NODE>


	<!-- The root node -->
    <NODE id="room" selectable="true">
      <MATERIAL id="defaultMaterial" />
      <TEXTURE id="null" />

	  <TRANSLATION x="-70" y="-8" z="-75" />
	  <SCALE sx="10" sy="10" sz="10" />

      <DESCENDANTS>
        <NODEREF id="floor" />
        <NODEREF id="walls" />
        <NODEREF id="carpet" />
        <NODEREF id="table_center" />
        </DESCENDANTS>
    </NODE>

    <!-- The floor -->
    <NODE id="floor">
      <MATERIAL id="floorMaterial" />
      <TEXTURE id="woodFloorTexture" />

      <TRANSLATION x="7.5" y="0" z="7.5" />
      <ROTATION axis="x" angle="-90" />
      <SCALE sx="15" sy="15" sz="1" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="-0.5 0.5 0.5 -0.5" />
      </DESCENDANTS>
    </NODE>

    <!-- The Walls -->
    <NODE id="walls">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <DESCENDANTS>
        <NODEREF id="wall_left" />
        <NODEREF id="wall_back" />
      </DESCENDANTS>
    </NODE>

    <!-- The Back Wall -->
    <NODE id="wall_back">
      <MATERIAL id="redWallMaterial" />
      <TEXTURE id="wallTexture" />

      <TRANSLATION x="7.5" y="2.5" z="0" />

      <DESCENDANTS>
        <NODEREF id="wall" />
      </DESCENDANTS>
    </NODE>

    <!-- The Left Wall -->
    <NODE id="wall_left">
      <MATERIAL id="null" />
      <TEXTURE id="wallTexture" />

      <TRANSLATION x="0" y="2.5" z="7.5" />
      <ROTATION axis="y" angle="90" />
      <ROTATION axis="z" angle="180" />

      <DESCENDANTS>
        <NODEREF id="wall" />
      </DESCENDANTS>
    </NODE>

    <!-- Wall -->
    <NODE id="wall">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <SCALE sx="15" sy="5" sz="0" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="-0.5 0.5 0.5 -0.5" />
      </DESCENDANTS>
    </NODE>

      <!-- Carpet -->
    <NODE id="carpet">
      <MATERIAL id="redWallMaterial" />
      <TEXTURE id="carpetTexture" />

      <TRANSLATION x="7" y="0.05" z="7.5" />
      <ROTATION axis="x" angle="-90" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="-3 4 3 -4" />
      </DESCENDANTS>
    </NODE>

      <!-- Table Center -->
    <NODE id="table_center">
      <MATERIAL id="woodMaterial" />
      <TEXTURE id="woodTexture" />

      <TRANSLATION x="7" y="0.5" z="7.5" />
      <SCALE sx="0.5" sy="0.5" sz="0.5" />
      <ROTATION axis="x" angle="180" />

      <DESCENDANTS>
        <NODEREF id="tc_base"/>
        <NODEREF id="tc_left"/>
        <NODEREF id="tc_right"/>
        <NODEREF id="tc_top"/>
      </DESCENDANTS>
    </NODE>

    <!-- Table Center part left -->
    <NODE id="tc_left">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0.1" z="3" />
      <SCALE sx="5" sy="1.5" sz="0.3" />

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>
    </NODE>

    <!-- Table Center part right -->
    <NODE id="tc_right">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0.1" z="-3" />
      <SCALE sx="5" sy="1.5" sz="0.3" />

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>
    </NODE>

    <!-- Table Center base -->
    <NODE id="tc_base">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="-0.5" z="0" />
      <SCALE sx="5" sy="0.3" sz="6" />

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>
    </NODE>

    <!-- Table Center top -->
    <NODE id="tc_top">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0.6" z="0" />
      <SCALE sx="5" sy="0.3" sz="6" />

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>
    </NODE>

    <!-- Center of table center-->
    <NODE id="center_tc">
      <MATERIAL id="glassMaterial" />
      <TEXTURE id="clear" />

      <ROTATION axis="x" angle="90" />
      <TRANSLATION x="0" y="0" z="1.3" />
      <SCALE sx="0.3" sy="0.3" sz="0.3" />

      <DESCENDANTS>
        <LEAF type="sphere" args="2 20 20" />
      </DESCENDANTS>
    </NODE>

    <!-- A pyramid -->
    <NODE id="pyramid" selectable="true">

      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="12" y="5.8" z="4.75" />
      <SCALE sx="0.4" sy="2" sz="0.4" />

      <DESCENDANTS>
        <NODEREF id="halfPyramid1" />
        <NODEREF id="halfPyramid2" />
        <NODEREF id="basePyramid" />
      </DESCENDANTS>
    </NODE>

    <!-- The pyramid base -->
    <NODE id="basePyramid">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <ROTATION axis="x" angle="90" />
      <TRANSLATION x="1" y="6" z="2.87" />
      <SCALE sx="2.8" sy="3.5" sz="1" />

      <DESCENDANTS>
        <LEAF  type="rectangle" args="0 4 5 0" />
      </DESCENDANTS>
    </NODE>

    <!-- Half a pyramid -->
    <NODE id="halfPyramid1">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <DESCENDANTS>
        <NODEREF id="halfPyramid" />
